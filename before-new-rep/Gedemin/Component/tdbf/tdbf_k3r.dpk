  dbf_fields in 'dbf_fields.pas',
  dbf_idxcur in 'dbf_idxcur.pas',
  dbf_idxfile in 'dbf_idxfile.pas',
  dbf_lang in 'dbf_lang.pas',
  dbf_memo in 'dbf_memo.pas',
  dbf_parser in 'dbf_parser.pas',
  dbf in 'dbf.pas',
  dbf_pgfile in 'dbf_pgfile.pas',
  dbf_prscore in 'dbf_prscore.pas',
  dbf_prsdef in 'dbf_prsdef.pas',
  dbf_prssupp in 'dbf_prssupp.pas',
  dbf_str in 'dbf_str.pas',
  dbf_wtil in 'dbf_wtil.pas';

