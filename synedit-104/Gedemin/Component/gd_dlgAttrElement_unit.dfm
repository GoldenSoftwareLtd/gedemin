  object ibuAttrSet: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  ID,'
      '  ATTRKEY,'
      '  PARENT,'
      '  NAME,'
      '  DISABLED,'
      '  AFULL,'
      '  ACHAG,'
      '  AVIEW'
      'from GD_ATTRSET '
      'where'
      '  ID = :ID')
    ModifySQL.Strings = (
      'update GD_ATTRSET'
      'set'
      '  ID = :ID,'
      '  ATTRKEY = :ATTRKEY,'
      '  PARENT = :PARENT,'
      '  NAME = :NAME,'
      '  DISABLED = :DISABLED,'
      '  AFULL = :AFULL,'
      '  ACHAG = :ACHAG,'
      '  AVIEW = :AVIEW'
      'where'
      '  ID = :OLD_ID')
    InsertSQL.Strings = (
      'insert into GD_ATTRSET'
      '  (ID, ATTRKEY, PARENT, NAME, DISABLED, AFULL, ACHAG, AVIEW)'
      'values'
      
        '  (:ID, :ATTRKEY, :PARENT, :NAME, :DISABLED, :AFULL, :ACHAG, :AV' +
        'IEW)')
    DeleteSQL.Strings = (
      'delete from GD_ATTRSET'
      'where'
      '  ID = :OLD_ID')
    Left = 250
  end
