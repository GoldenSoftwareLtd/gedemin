  object ibdsCattle: TIBDataSet
    Database = dmDatabase.ibdbGAdmin
    Transaction = ibtrCattle
    ForcedRefresh = True
    BufferChunks = 1000
    CachedUpdates = False
    DeleteSQL.Strings = (
      'delete from GD_GOOD'
      'where'
      '  ID = :OLD_ID')
    InsertSQL.Strings = (
      'insert into GD_GOOD'
      '  (ID, GROUPKEY, NAME, ALIAS, SHORTNAME, DESCRIPTION, BARCODE, '
      'VALUEKEY, '
      '   TNVDKEY, ISASSEMBLY, RESERVED, DISABLED, AFULL, ACHAG, AVIEW)'
      'values'
      
        '  (:ID, :GROUPKEY, :NAME, :ALIAS, :SHORTNAME, :DESCRIPTION, :BAR' +
        'CODE, '
      ':VALUEKEY, '
      
        '   :TNVDKEY, :ISASSEMBLY, :RESERVED, :DISABLED, :AFULL, :ACHAG, ' +
        ':AVIEW)')
    RefreshSQL.Strings = (
      'SELECT'
      '  G.*, D.DISCOUNT, D.MINWEIGHT, D.MAXWEIGHT, D.COEFF'
      ''
      'FROM'
      '  GD_GOOD G'
      '    JOIN'
      '      GD_GOODGROUP GG'
      '    ON'
      '      GG.ID = G.GROUPKEY'
      '    LEFT JOIN'
      '      CTL_DISCOUNT D'
      '    ON'
      '      D.GOODKEY = G.ID'
      ''
      'WHERE'
      '  G.ID = :ID')
    SelectSQL.Strings = (
      'SELECT'
      '  G.*, D.DISCOUNT, D.MINWEIGHT, D.MAXWEIGHT, D.COEFF'
      ''
      'FROM'
      '  GD_GOOD G'
      '    JOIN'
      '      GD_GOODGROUP GG'
      '    ON'
      '      GG.ID = G.GROUPKEY'
      '    LEFT JOIN'
      '      CTL_DISCOUNT D'
      '    ON'
      '      D.GOODKEY = G.ID'
      ''
      'WHERE'
      '  GG.LB >= :LB'
      '    AND'
      '  GG.RB <= :RB'
      ' ')
    ModifySQL.Strings = (
      'update GD_GOOD'
      'set'
      '  ID = :ID,'
      '  GROUPKEY = :GROUPKEY,'
      '  NAME = :NAME,'
      '  ALIAS = :ALIAS,'
      '  SHORTNAME = :SHORTNAME,'
      '  DESCRIPTION = :DESCRIPTION,'
      '  BARCODE = :BARCODE,'
      '  VALUEKEY = :VALUEKEY,'
      '  TNVDKEY = :TNVDKEY,'
      '  ISASSEMBLY = :ISASSEMBLY,'
      '  RESERVED = :RESERVED,'
      '  DISABLED = :DISABLED,'
      '  AFULL = :AFULL,'
      '  ACHAG = :ACHAG,'
      '  AVIEW = :AVIEW'
      'where'
      '  ID = :OLD_ID')
    Left = 160
    Top = 70
  end
